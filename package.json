  "scripts": {
    "test": "jest --coverage",
    "flow": "flow",
    "eject": "react-scripts eject",
    "watch": "nodemon --ext *",
    "init": "npm install && npm i -g eslint && npm i -g eslint-plugin-react && npm i -g npm-check-updates",
    "release": "npm run prebuild ",
    "clean": "rimraf dist/*",
    "lint": "eslint src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build0": "npm run lint && npm run flow && npm run eject",
    "prestart": "npm run clean",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "pre-push": "npm run test",
    "prepublish": "npm run build",
    "runserver": "cross-env NODE_ENV=production node server/www.js",
    "runproject": "cross-env NODE_ENV=development nodemon server/www.js",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:e2e": "cross-env NODE_ENV=test jest --configured",
    "dev": "concurrently \"npm run  runproject \" \"npm run start \"",
    "production": "concurrently \"npm run runserver\" \"npm run serve \"",
    "serve": "serve -s build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguemechieu/tradeadviser.git",
